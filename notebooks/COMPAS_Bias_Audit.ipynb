    "# COMPAS Bias Audit Notebook\n",
    "This notebook demonstrates analyzing racial bias in recidivism prediction using AI Fairness 360."
   ]
  },
  {
   "cell_type": "code",
   "metadata": {},
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from aif360.datasets import BinaryLabelDataset\n",
    "from aif360.metrics import ClassificationMetric\n",
    "\n",
    "# Load dataset\n",
    "df = pd.read_csv('../data/compas_dataset.csv')\n",
    "df.head()"
   ]
  },
  {
   "cell_type": "code",
   "metadata": {},
   "source": [
    "# Create BinaryLabelDataset\n",
    "dataset = BinaryLabelDataset(\n",
    "    df=df,\n",
    "    label_names=['two_year_recid'],\n",
    "    protected_attribute_names=['race']\n",
    ")\n",
    "\n",
    "# Example: visualize disparity\n",
    "counts = df.groupby('race')['two_year_recid'].mean()\n",
    "counts.plot(kind='bar', title='Recidivism by Race')\n",
    "plt.ylabel('Mean Recidivism Rate')\n",
    "plt.show()"
   ]
  }
